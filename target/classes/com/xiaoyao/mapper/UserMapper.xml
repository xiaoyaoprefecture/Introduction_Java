<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiaoyao.dao.UserDao">
	<!--查询所有  -->
    <select id="selectAll" resultType="aaa">
        select * from user_tb
    </select>
    
    <!-- 添加用户 -->
    <insert id="addUser" parameterType="aaa">
    	insert into user_tb values(#{userName},#{userPassword},#{userAccount})
    </insert>
    
    <!-- 查询是否存在当前用户名的用户 -->
    <select id="selectUserByUserName" resultType="aaa" parameterType="aaa">
        select * from user_tb where userName=#{userName}
    </select>
    
</mapper>